#!/usr/bin/dash
/bin/mkdir -p /tmp/__ram_bin__
cp -rf $(find $HOME/.local/bin -type f ! -path '*git*' ! -path '*vscode*') /tmp/__ram_bin__ 2> /dev/null &
__load_bin__() { cp -rf $@ /tmp/__ram_bin__ 2> /dev/null;}
__load_bin__ /usr/bin/alacritty &
__load_bin__ /usr/bin/nvim &
__load_bin__ /usr/bin/zsh &
__load_bin__ /usr/local/bin/dmenu &
__load_bin__ /usr/local/bin/dmenu_run &
__load_bin__ /usr/local/bin/dmenu_path &
unset __load_bin__
